
<% if user_signed_in? %>

<h1> Hi There <%=  current_user.name %> </h1> <br>



<h2> Here are your posts <%=  current_user.id  %></h2>


  <% current_user.posts.each do |post| %>
    <%= post.title%> <br>
    <%= post.body%> <br>
    <%end%>

<%= link_to "Sign Out", destroy_user_session_path, 'data-turbo-method': :delete%>


<%else%>
<h1> Not signed in</h1>

 <%= link_to "Log in", "/users/sign_in" %><br />

<%end%>






